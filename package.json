{
    "name": "mnml-web",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "serve": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test": "run-p test:*",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "format": "npm-run-all prettier lint",
        "prettier": "prettier --write .",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "eslint . --fix",
        "lint:style": "stylelint '**/*.{vue,htm,html,sass}' --cache"
    },
    "dependencies": {
        "compute-lcm": "~1.1.2",
        "throttle-debounce": "~5.0.2",
        "vanilla-framework": "~4.22.0",
        "vue": "~3.5.13",
        "webmidi": "~3.1.12"
    },
    "devDependencies": {
        "@commitlint/cli": "~19.8.0",
        "@commitlint/config-conventional": "~19.8.0",
        "@emm-ess-configs/eslint-config-vue": "^0.2.3",
        "@eslint/compat": "~1.2.7",
        "@eslint/js": "~9.23.0",
        "@semantic-release/changelog": "~6.0.3",
        "@semantic-release/git": "~10.0.1",
        "@tsconfig/node22": "^22.0.1",
        "@tsconfig/strictest": "^2.0.5",
        "@types/compute-lcm": "^1.1.3",
        "@types/jsdom": "~21.1.7",
        "@types/node": "~20.14.8",
        "@types/throttle-debounce": "~5.0.2",
        "@vitejs/plugin-vue": "~5.2.3",
        "@vue/test-utils": "~2.4.6",
        "@vue/tsconfig": "~0.7.0",
        "browserslist-to-esbuild": "^2.1.1",
        "eslint": "~9.23.0",
        "husky": "~9.1.7",
        "imagemin-lint-staged": "~0.5.1",
        "jsdom": "~26.0.0",
        "lint-staged": "~15.5.0",
        "npm-run-all": "~4.1.5",
        "postcss-html": "~1.8.0",
        "postcss-sass": "~0.5.0",
        "prettier": "~3.5.3",
        "sass": "~1.86.1",
        "semantic-release": "~24.2.3",
        "stylelint": "~16.17.0",
        "stylelint-config-outside-in-order": "~0.1.1",
        "stylelint-config-sass-guidelines": "~12.1.0",
        "stylelint-config-standard": "~37.0.0",
        "stylelint-declaration-block-no-ignored-properties": "~2.8.0",
        "stylelint-declaration-strict-value": "~1.10.11",
        "stylelint-high-performance-animation": "~1.11.0",
        "typescript": "~5.8.2",
        "vite": "~6.2.4",
        "vite-plugin-mkcert": "~1.17.8",
        "vitest": "~3.1.1",
        "vue-tsc": "~2.2.8"
    },
    "overrides": {
        "eslint": "~9.23.0"
    }
}
