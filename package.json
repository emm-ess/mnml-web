{
    "name": "mnml-web",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "serve": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "format": "npm-run-all prettier lint",
        "prettier": "prettier --write .",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json --fix --ignore-path .gitignore",
        "lint:style": "stylelint '**/*.{vue,htm,html,sass}' --cache"
    },
    "dependencies": {
        "throttle-debounce": "^5.0.0",
        "vanilla-framework": "^3.12.1",
        "vue": "^3.2.47",
        "webmidi": "^3.1.5"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.4",
        "@commitlint/config-conventional": "17.4.4",
        "@rushstack/eslint-patch": "^1.2.0",
        "@semantic-release/changelog": "6.0.2",
        "@semantic-release/git": "10.0.1",
        "@types/jsdom": "^21.1.0",
        "@types/node": "^18.15.0",
        "@types/throttle-debounce": "^5.0.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.3.1",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "8.36.0",
        "eslint-plugin-compat": "^4.1.2",
        "eslint-plugin-deprecation": "1.3.3",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-json": "3.1.0",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sonarjs": "0.18.0",
        "eslint-plugin-unicorn": "46.0.0",
        "eslint-plugin-vue": "^9.9.0",
        "husky": "^8.0.3",
        "imagemin-lint-staged": "0.5.1",
        "jsdom": "^21.1.0",
        "lint-staged": "13.2.0",
        "npm-run-all": "^4.1.5",
        "postcss-html": "1.5.0",
        "postcss-sass": "0.5.0",
        "prettier": "^2.8.4",
        "sass": "^1.59.2",
        "semantic-release": "20.1.1",
        "stylelint": "14.16.1",
        "stylelint-config-outside-in-order": "0.1.0",
        "stylelint-config-sass-guidelines": "9.0.1",
        "stylelint-config-standard": "30.0.1",
        "stylelint-declaration-block-no-ignored-properties": "2.7.0",
        "stylelint-declaration-strict-value": "1.9.2",
        "stylelint-high-performance-animation": "1.8.0",
        "stylelint-no-unsupported-browser-features": "6.1.0",
        "typescript": "4.9.5",
        "vite": "^4.1.4",
        "vite-plugin-checker": "0.5.6",
        "vite-plugin-mkcert": "^1.13.3",
        "vitest": "^0.29.2",
        "vue-class-component": "^8.0.0-0",
        "vue-property-decorator": "^10.0.0-0",
        "vue-tsc": "^1.2.0"
    }
}
