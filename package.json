{
    "name": "mnml-web",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "serve": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "format": "npm-run-all prettier lint",
        "prettier": "prettier --write .",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json --fix --ignore-path .gitignore",
        "lint:style": "stylelint '**/*.{vue,htm,html,sass}' --cache"
    },
    "dependencies": {
        "throttle-debounce": "^5.0.0",
        "vanilla-framework": "^3.14.0",
        "vue": "^3.3.4",
        "webmidi": "^3.1.5"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.3",
        "@commitlint/config-conventional": "17.6.3",
        "@rushstack/eslint-patch": "^1.2.0",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@tsconfig/node18": "^2.0.1",
        "@types/jsdom": "^21.1.1",
        "@types/node": "^20.2.1",
        "@types/throttle-debounce": "^5.0.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.3.2",
        "@vue/tsconfig": "^0.4.0",
        "eslint": "8.40.0",
        "eslint-plugin-compat": "^4.1.4",
        "eslint-plugin-deprecation": "1.4.1",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-json": "3.1.0",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sonarjs": "0.19.0",
        "eslint-plugin-unicorn": "47.0.0",
        "eslint-plugin-vue": "^9.13.0",
        "husky": "^8.0.3",
        "imagemin-lint-staged": "0.5.1",
        "jsdom": "^22.0.0",
        "lint-staged": "13.2.2",
        "npm-run-all": "^4.1.5",
        "postcss-html": "1.5.0",
        "postcss-sass": "0.5.0",
        "prettier": "^2.8.8",
        "sass": "^1.62.1",
        "semantic-release": "21.0.2",
        "stylelint": "15.6.2",
        "stylelint-config-outside-in-order": "0.1.0",
        "stylelint-config-sass-guidelines": "10.0.0",
        "stylelint-config-standard": "33.0.0",
        "stylelint-declaration-block-no-ignored-properties": "2.7.0",
        "stylelint-declaration-strict-value": "1.9.2",
        "stylelint-high-performance-animation": "1.8.0",
        "stylelint-no-unsupported-browser-features": "6.1.0",
        "typescript": "5.0.4",
        "vite": "^4.3.8",
        "vite-plugin-checker": "0.6.0",
        "vite-plugin-mkcert": "^1.15.0",
        "vitest": "^0.31.1",
        "vue-class-component": "^8.0.0-0",
        "vue-property-decorator": "^10.0.0-0",
        "vue-tsc": "^1.6.5"
    }
}
