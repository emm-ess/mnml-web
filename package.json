{
    "name": "mnml-web",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "serve": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "format": "npm-run-all prettier lint",
        "prettier": "prettier --write .",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "eslint . --fix",
        "lint:style": "stylelint '**/*.{vue,htm,html,sass}' --cache"
    },
    "dependencies": {
        "compute-lcm": "~1.1.2",
        "throttle-debounce": "~5.0.2",
        "vanilla-framework": "~4.18.0",
        "vue": "~3.5.12",
        "webmidi": "~3.1.11"
    },
    "devDependencies": {
        "@commitlint/cli": "~19.5.0",
        "@commitlint/config-conventional": "~19.5.0",
        "@emm-ess-configs/eslint-config-vue": "~0.2.1",
        "@eslint/compat": "~1.2.1",
        "@eslint/js": "~9.13.0",
        "@semantic-release/changelog": "~6.0.3",
        "@semantic-release/git": "~10.0.1",
        "@tsconfig/node20": "^20.1.4",
        "@tsconfig/strictest": "^2.0.5",
        "@types/compute-lcm": "^1.1.3",
        "@types/jsdom": "~21.1.7",
        "@types/node": "~20.14.8",
        "@types/throttle-debounce": "~5.0.2",
        "@vitejs/plugin-vue": "~5.1.4",
        "@vue/test-utils": "~2.4.6",
        "@vue/tsconfig": "~0.5.1",
        "browserslist-to-esbuild": "^2.1.1",
        "eslint": "~9.19.0",
        "husky": "~9.1.6",
        "imagemin-lint-staged": "~0.5.1",
        "jsdom": "~25.0.1",
        "lint-staged": "~15.2.10",
        "npm-run-all": "~4.1.5",
        "postcss-html": "~1.7.0",
        "postcss-sass": "~0.5.0",
        "prettier": "~3.3.3",
        "sass": "~1.80.4",
        "semantic-release": "~24.2.0",
        "stylelint": "~16.10.0",
        "stylelint-config-outside-in-order": "~0.1.0",
        "stylelint-config-sass-guidelines": "~12.1.0",
        "stylelint-config-standard": "~36.0.1",
        "stylelint-declaration-block-no-ignored-properties": "~2.8.0",
        "stylelint-declaration-strict-value": "~1.10.6",
        "stylelint-high-performance-animation": "~1.10.0",
        "typescript": "~5.6.3",
        "vite": "~5.4.10",
        "vite-plugin-checker": "~0.8.0",
        "vite-plugin-mkcert": "~1.17.6",
        "vitest": "~2.1.3",
        "vue-tsc": "~2.1.6"
    },
    "overrides": {
        "eslint": "~9.19.0",
        "stylelint-config-outside-in-order": {
            "stylelint": "~16.10.0"
        }
    }
}
